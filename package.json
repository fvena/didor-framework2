    "lint:scss": "sass-lint --config sass-lint.yml -v -q"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvena/didor-framework.git"
  },
  "bugs": {
    "url": "https://github.com/fvena/didor-framework/issues"
  },
  "devDependencies": {
    "docsify-cli": "^4.2.1",
    "node-sass": "4.9.0",
    "sass-lint": "1.12.1"
  }
}
